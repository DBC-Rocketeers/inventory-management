
  <table class="inventory-table">
    <tr class="header-row">
        <th class="header-th">Part Name</th>
        <th class="header-th">Part Number</th>
        <th class="header-th">Quantity</th>
    </tr>
    <% inventory.each do |part| %>
      <tr class="inventory-tr">
        <th class="inventory-th"><%= part.name %></th>
        <th class="inventory-th"><%= part.number %></th>
        <th class="inventory-th"><%= part.local_count(location) %></th>
      </tr>
    <% end %>
  </table>
